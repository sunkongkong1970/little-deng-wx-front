.page{padding:16px}
.empty{display:flex;flex-direction:column;align-items:center;margin-top:40px}
.dashed{width:200px;height:200px;border:2px dashed #ccc;border-radius:12px;display:flex;align-items:center;justify-content:center;font-size:48px;color:#07C160}
.filled{display:flex;flex-direction:column;align-items:center;gap:12px}
.info{background:#FFF5E6;padding:10px;border-radius:8px;margin-top:8px}
.name{font-weight:600}
.meta{color:#999;font-size:12px;margin-left:6px}
.edit{color:#07C160}
